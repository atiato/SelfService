  "scripts": {
    "test": "webpack --config webpack.spec.ts --progress --color && karma start",
    "start": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --content-base src",
    "deploy": "webpack --optimize-minimize --define process.env.NODE_ENV='prod",
    "api": "angularclass-server --api retain --port 3500",
    "clean": "del -rf dist",
    "serve": "http-server . -p 5556",
    "build": "npm run clean && tsc",
    "build_prod": "npm run build && browserify -s main dist/main.js > dist/bundle.js && npm run minify",
