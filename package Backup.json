    "@angular/http": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "@angular/platform-browser-dynamic": "^2.0.0",
    "@angular/router": "^3.0.0",
    "@angularclass/api": "^1.2.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "@progress/kendo-angular-buttons": "^0.14.2",
    "@progress/kendo-angular-grid": "^0.8.2",
    "@progress/kendo-data-query": "^0.3.4",
    "@progress/kendo-theme-default": "^2.1.2",
    "angular2localization": "^1.4.1",
